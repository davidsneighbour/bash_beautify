{
    "name": "bash-beautify",
    "displayName": "Bash Beautify",
    "description": "Format / Beautify bash scripts",
    "version": "0.0.3",
    "repository": {
        "type": "Git",
        "url": "https://github.com/shakram02/shell_beautify"
    },
    "galleryBanner": {
        "color": "#ffc73b",
        "theme": "dark"
    },
    "author": {
        "email": "ahmedhamdyau@gmail.com",
        "name": "Ahmed Hamdy (shakram02)",
        "url": "https://github.com/shakram02"
    },
    "keywords": [
        "shell script",
        "bash script",
        "bash",
        "formatter",
        "beautifier"
    ],
    "publisher": "shakram02",
    "license": "MIT",
    "homepage": "https://github.com/shakram02/shell_beautify",
    "icon": "images/shell-beautify.png",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Formatters"
    ],
    "contributes": {
        "languages": [
            {
                "id": "shellscript",
                "extensions": [
                    ".sh"
                ]
            }
        ]
    },
    "activationEvents": [
        "onLanguage:shellscript"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "child-process-promise": "^2.2.1",
        "gulp": "^3.9.1",
        "gulp-typescript": "^3.1.7"
    }
}